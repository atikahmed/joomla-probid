<?xml version="1.0" encoding="utf-8"?>
<layout name="desktop">
	<!--Extra css load for this layout-->
	<stylesheets>
		<file>css/jquery.fancybox-1.3.4.css</file>
		<file>css/mootools.css</file>
		<file>css/layout_import.css</file>
		<file>css/layout_tab_slide.css</file>
	</stylesheets>
	<blocks name="top" style="xhtml">
		<block name="absolute" type="modules" style="raw">absolute</block>
		<block name="top-panel" type="modules" style="raw" main-inner="1">top-panel</block>
		<block name="header" type="header" main-inner="1"></block>
		<block name="mainnav" type="mainnav" main-inner="1"></block>
		<block name="cpanel" type="usertools/cpanel"></block>
                 <block name="feature" type="spotlight" main-inner="1">feature</block>	
		<block name="topsl" type="spotlight" main-inner="1">user1,user2,user3,user4,user5</block>
                <block name="utility" type="spotlight" main-inner="1">utility</block>	
	</blocks>
        <blocks name="middle" colwidth="20" fixheight="1">
          <block name="right-mass-top" >right-mass-top</block>
          <block name="content-mass-top">content-mass-top</block>
          <block name="right1">left</block>
          <block name="right2">right</block>
      </blocks>
      <blocks name="mainbottoms" style="xhtml">
		<block name="mainbottom" type="spotlight" main-inner="1">mainbottom1, mainbottom2, mainbottom3</block>
	</blocks>
	<blocks name="bottom" style="xhtml">
		<block name="bottom" type="spotlight" main-inner="1">bottom</block>
               <block name="footer" type="spotlight" main-inner="1">footer1, footer2, footer3, footer4</block>
		<block name="copyright" type="copyright"></block>
	</blocks>
	<!--Extra JS load for this layout --> 
	<scripts>
		<file>js/jquery.fancybox-1.3.4.js</file>
		<file>js/slideGallery.js</file>
		<file>js/hide.js</file>
	</scripts>
</layout>