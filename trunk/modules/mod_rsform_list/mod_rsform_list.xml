<?xml version="1.0" encoding="utf-8"?>
<install type="module" version="1.5" method="upgrade" client="site">
	<name>RSForm! Pro Module Frontend List</name>
	<author>RSJoomla</author>
	<creationDate>July 2010</creationDate>
	<copyright>(C) 2007-2010 www.rsjoomla.com</copyright>
	<license>GNU General Public License</license>
	<authorEmail>support@rsjoomla.com</authorEmail>
	<authorUrl>www.rsjoomla.com</authorUrl>
	<version>1.3.0</version>
	<description><![CDATA[MOD_RSFORMLIST_DESCR]]></description>
	<files>
		<filename module="mod_rsform_list">mod_rsform_list.php</filename>
		<filename>helper.php</filename>
		<filename>index.html</filename>
		<folder>tmpl</folder>
	</files>
	<languages>
		<language tag="en-GB">en-GB.mod_rsform_list.ini</language>
	</languages>
	<params addPath="/administrator/components/com_rsform/elements">
		<param name="formId" type="text" default="1" label="MOD_RSFORMLIST_ID" description="MOD_RSFORMLIST_ID_DESC" />
		<param name="lang" type="lang" default="" label="MOD_RSFORMLIST_SHOW_SUBMISSIONS_LANGUAGE" description="MOD_RSFORMLIST_SHOW_SUBMISSIONS_LANGUAGE_DESC" />
		<param name="sort_submissions" type="radio" default="0" label="MOD_RSFORMLIST_SORT_SUBMISSIONS" description="MOD_RSFORMLIST_SORT_SUBMISSIONS_DESC">
				<option value="0">MOD_RSFORMLIST_DESC</option>
				<option value="1">MOD_RSFORMLIST_ASC</option>
		</param>
		<param name="@spacer" type="spacer" default="" label="" description="" />
		<param name="userId" type="text" default="0" label="MOD_RSFORMLIST_SHOW_SUBMISSIONS_FOR" description="MOD_RSFORMLIST_SHOW_SUBMISSIONS_FOR_DESC" />
		<param name="show_confirmed" type="radio" default="0" label="MOD_RSFORMLIST_SHOW_CONFIRMED_SUBMISSIONS" description="MOD_RSFORMLIST_SHOW_CONFIRMED_SUBMISSIONS_DESC">
			<option value="0">NO</option>
			<option value="1">YES</option>
		</param>
		<param name="template_module" type="textarea" default="&lt;table width=&quot;100%&quot;&gt;{formdata}&lt;/table&gt;" label="MOD_RSFORMLIST_MODULE_LAYOUT" description="MOD_RSFORMLIST_MODULE_LAYOUT_DESC" cols="60" rows="7"/>
		<param name="template_formdatarow" type="textarea" default="&lt;tr&gt;&lt;td&gt;{details}{FullName:value}{/details}&lt;/td&gt;&lt;td&gt;{Email:value}&lt;/td&gt;&lt;/tr&gt;" label="MOD_RSFORMLIST_ROW_LAYOUT" description="MOD_RSFORMLIST_ROW_LAYOUT_DESC" cols="60" rows="7" />
		<param name="template_formdetail" type="textarea" default="&lt;table width=&quot;100%&quot;&gt;&lt;tr&gt;&lt;td&gt;{FullName:value}&lt;/td&gt;&lt;td&gt;{Email:value}&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;" label="MOD_RSFORMLIST_DETAILS_LAYOUT" description="MOD_RSFORMLIST_DETAILS_LAYOUT_DESC" cols="60" rows="7" />
		<param name="limit" type="text" default="30" label="MOD_RSFORMLIST_PAGE_LIMIT" description="MOD_RSFORMLIST_PAGE_LIMIT_DESC" />
	</params>
	<config>
		<fields name="params">
			<fieldset name="basic">
				<field name="formId" type="text" default="1" label="MOD_RSFORMLIST_ID" description="MOD_RSFORMLIST_ID_DESC" />
				<field name="lang" type="lang" default="" label="MOD_RSFORMLIST_SHOW_SUBMISSIONS_LANGUAGE" description="MOD_RSFORMLIST_SHOW_SUBMISSIONS_LANGUAGE_DESC" addfieldpath="/administrator/components/com_rsform/fields" />
				<field name="sort_submissions" type="radio" default="0" label="MOD_RSFORMLIST_SORT_SUBMISSIONS" description="MOD_RSFORMLIST_SORT_SUBMISSIONS_DESC">
					<option value="0">MOD_RSFORMLIST_DESC</option>
					<option value="1">MOD_RSFORMLIST_ASC</option>
				</field>
				<field name="@spacer" type="spacer" default="" label="" description="" />
				<field name="userId" type="text" default="0" label="MOD_RSFORMLIST_SHOW_SUBMISSIONS_FOR" description="MOD_RSFORMLIST_SHOW_SUBMISSIONS_FOR_DESC" />
				<field name="show_confirmed" type="radio" default="0" label="MOD_RSFORMLIST_SHOW_CONFIRMED_SUBMISSIONS" description="MOD_RSFORMLIST_SHOW_CONFIRMED_SUBMISSIONS_DESC">
					<option value="0">NO</option>
					<option value="1">YES</option>
				</field>
				<field filter="raw" name="template_module" type="textarea" default="&lt;table width=&quot;100%&quot;&gt;{formdata}&lt;/table&gt;" label="MOD_RSFORMLIST_MODULE_LAYOUT" description="MOD_RSFORMLIST_MODULE_LAYOUT_DESC" cols="60" rows="7" />
				<field filter="raw" name="template_formdatarow" type="textarea" default="&lt;tr&gt;&lt;td&gt;{details}{FullName:value}{/details}&lt;/td&gt;&lt;td&gt;{Email:value}&lt;/td&gt;&lt;/tr&gt;" label="MOD_RSFORMLIST_ROW_LAYOUT" description="MOD_RSFORMLIST_ROW_LAYOUT_DESC" cols="60" rows="7" />
				<field filter="raw" name="template_formdetail" type="textarea" default="&lt;table width=&quot;100%&quot;&gt;&lt;tr&gt;&lt;td&gt;{FullName:value}&lt;/td&gt;&lt;td&gt;{Email:value}&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;" label="MOD_RSFORMLIST_DETAILS_LAYOUT" description="MOD_RSFORMLIST_DETAILS_LAYOUT_DESC" cols="60" rows="7" />
				<field name="limit" type="text" default="30" label="MOD_RSFORMLIST_PAGE_LIMIT" description="MOD_RSFORMLIST_PAGE_LIMIT_DESC" />
			</fieldset>
		</fields>
	</config>
</install>